{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react",
      "lib": ["es2017", "dom"],
      "moduleResolution": "node",
      "noEmit": true,
      "strict": true,
      "target": "es5",
      "module": "CommonJS"
    },
    "exclude": [
      "node_modules",
    ]
  }